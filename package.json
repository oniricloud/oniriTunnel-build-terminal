{
  "name": "oniri-tunnel-terminal",
  "bin": "index.js",
  "pear": {
    "name": "oniri-tunnel-terminal",
    "type": "terminal",
    "routes": ".",
    "unrouted": [
      "/services/oniri-core/src/app.js"
    ],
    "stage": {
      "entrypoints": [
        "services/oniri-core/src/app.js"
      ],
      "ignore": [
        ".github",
        "appling",
        ".git",
        ".gitignore"
      ]
    }
  },
  "type": "module",
  "license": "Apache-2.0",
  "scripts": {
    "dev": "pear run -d .",
    "dev:server": "bare ./server.js",
    "dev:client": "bare ./client.js",
    "test": "brittle test/*.test.js",
    "install:all": "npm install && npm install --prefix services/oniri-core && npm install --prefix appling",
    "build": "bare-build --standalone --host darwin-arm64 --out build index.js"
  },
  "devDependencies": {
    "bare-make": "^1.6.3",
    "brittle": "^3.0.0",
    "cmake-bare-bundle": "^2.1.5",
    "cmake-fetch": "^1.4.8",
    "pear-interface": "^1.0.0"
  },
  "dependencies": {
    "bare-build": "^0.4.1",
    "bare-daemon": "^1.2.4",
    "bare-events": "^2.8.2",
    "bare-http1": "^4.2.2",
    "bare-https": "^2.1.2",
    "bare-ipc": "^1.1.1",
    "bare-pipe": "^4.1.2",
    "bare-readline": "^1.2.1",
    "bare-rpc": "^1.1.0",
    "bare-tty": "^5.0.3",
    "bare-url": "^2.3.2",
    "framed-stream": "^1.0.1",
    "minimist": "^1.2.8",
    "oniri-core": "file:services/oniri-core",
    "paparam": "^1.10.0",
    "pear-ipc": "^6.9.0",
    "ready-resource": "^1.2.0",
    "which-runtime": "^1.3.2"
  }
}
